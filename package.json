{
  "name": "bio-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "compile-css": "sass --load-path=node_modules --no-source-map src/global.scss src/global.css",
    "dev": "vite dev",
    "dev:inspect": "vavite-loader vite dev",
    "format": "prettier ./src --write .",
    "lint": "eslint ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@sveltejs/kit": "~1.20.5",
    "carbon-components": "~10.58.3",
    "carbon-components-svelte": "~0.76.0",
    "carbon-icons-svelte": "~11.4.0",
    "http-status-codes": "~2.2.0",
    "jwt-decode": "~3.1.2",
    "rxjs": "~7.8.1",
    "sveltekit-flash-message": "~1.0.1",
    "tslib": "~2.5.3",
    "type-fest": "~3.12.0",
    "uuid": "~9.0.0",
    "zod": "~3.21.4",
    "zod-form-data": "~2.0.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "~1.2.4",
    "@types/node": "~18.16.3",
    "@types/uuid": "~9.0.2",
    "@typescript-eslint/eslint-plugin": "~5.60.0",
    "@typescript-eslint/parser": "~5.60.0",
    "autoprefixer": "~10.4.14",
    "carbon-preprocess-svelte": "~0.9.1",
    "eslint": "~8.43.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-svelte3": "~4.0.0",
    "postcss": "~8.4.24",
    "postcss-load-config": "~4.0.1",
    "postcss-scss": "^4.0.6",
    "prettier": "~2.8.8",
    "prettier-plugin-svelte": "~2.10.1",
    "sass": "~1.63.6",
    "svelte": "~3.59.1",
    "svelte-check": "~3.4.4",
    "svelte-preprocess": "~5.0.4",
    "tailwindcss": "~3.3.2",
    "typescript": "~5.1.3",
    "vite": "~4.3.9"
  }
}
